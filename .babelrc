//{
//  "presets": ["es2015", "react"]
//}
{
  "presets": [
    //"@babel/preset-env",
    //"@babel/preset-react"
    [
      "env",
      {
        "targets": {
          "browsers": [
            "Chrome >= 52",
            "FireFox >= 44",
            "Safari >= 7",
            "Explorer 11",
            "last 4 Edge versions"
          ]
        },
        "useBuiltIns": true
      }
    ],
    "react",
    "stage-1"
  ],
  "ignore": [
    "node_modules"
  ],
  "plugins": [
    //[ "@babel/plugin-proposal-decorators", { "legacy": true } ],
    "transform-decorators-legacy",
    "transform-class-properties",
    "syntax-class-properties"
  ]
}